{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "env": {
        "browser": true,
        "node": true
    },
    "rules": {
        "comma-dangle": ["error", "never"],
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "no-unused-vars": ["warn","all"],
        "no-use-before-define": ["error", "nofunc"],
        "arrow-body-style": ["error", "as-needed", {"requireReturnForObjectLiteral": true }]
    }
}
